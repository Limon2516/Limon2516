/* obtiene la tabla de multiplicar de un numero//*/
# include <stdio.h>

int main () {
	
	int numero ;
	
	printf("Ingrese el numero:");
	scanf("%d", & numero);
	
	printf("tabla de multiplicar de %d\n", numero);
		for (int i = 1; i<= 10; i++)
	{
		printf("%d x %d = %d\n", numero, i, numero * i);
	}
	
	return 0;
}
